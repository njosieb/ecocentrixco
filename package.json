{
  "name": "ecocentrixco",
  "version": "0.0.1",
  "license": "MIT",
  "dependencies": {
    "font-awesome": "^4.7.0",
    "jquery": "2.2.0",
    "lodash": "4.17.2",
    "ng-file-upload": "12.2.x",
    "normalize.css": "3.0.3",
    "sugar": "2.0.4",
    "tachyons": "~4.8.1",
    "waypoints": "4.0.1"
  },
  "devDependencies": {
    "autoprefixer": "6.3.1",
    "babel-core": "6.9.1",
    "babel-eslint": "6.0.4",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-preset-es2015": "6.9.0",
    "babelify": "7.3.0",
    "browser-sync": "2.13.0",
    "browserify": "13.0.1",
    "browserify-shim": "3.8.12",
    "del": "2.2.0",
    "envify": "3.4.0",
    "file-saver": "1.3.3",
    "foldero": "0.1.1",
    "gulp-gh-pages": "^0.5.4",
    "gulp": "3.9.0",
    "gulp-angular-templatecache": "1.8.0",
    "gulp-changed": "1.3.0",
    "gulp-data": "1.2.1",
    "gulp-eslint": "1.1.1",
    "gulp-fontmin": "0.7.4",
    "gulp-htmlmin": "1.3.0",
    "gulp-if": "2.0.0",
    "gulp-imagemin": "2.4.0",
    "gulp-less": "3.0.5",
    "gulp-load-plugins": "1.2.0",
    "gulp-newer": "1.1.0",
    "gulp-ng-annotate": "2.0.0",
    "gulp-notify": "2.2.0",
    "gulp-nunjucks-html": "2.0.0",
    "gulp-plumber": "1.0.1",
    "gulp-postcss": "6.0.1",
    "gulp-rename": "1.2.2",
    "gulp-sourcemaps": "1.6.0",
    "gulp-uglify": "1.5.1",
    "gulp-util": "3.0.7",
    "glob": "6.0.4",
    "imagemin-pngquant": "4.2.0",
    "imagemin-svgo": "4.2.0",
    "js-yaml": "3.5.2",
    "less": "2.7.2",
    "minimist": "1.2.0",
    "nunjucks": "2.4.0",
    "pluto-path": "0.3.0",
    "rimraf": "2.5.0",
    "vinyl-buffer": "1.0.0",
    "vinyl-source-stream": "1.1.0",
    "watchify": "3.7.0 ",
    "wrench": "1.5.8"
  },
  "scripts": {
    "postinstall": "gulp build --production"
  },
  "engines": {
    "node": "6.9.1"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browser": {
    "waypoints": "./node_modules/waypoints/lib/jquery.waypoints.min.js"
  },
  "browserify-shim": {
    "waypoints": {
      "exports": "waypoints",
      "depends": "jquery:jQuery"
    }
  },
  "//": "CUSTOM CONFIGURATION",
  "config": {
    "//": "Entry files",
    "host": "127.0.0.1",
    "port": "3000",
    "baseUrl": "/ecocentrixco",
    "directories": {
      "source": "src",
      "destination": "build",
      "temporary": "tmp",
      "//": "Directories relative to `source` directory",
      "modules": "_modules",
      "layouts": "_layouts",
      "fonts": "_fonts",
      "images": "_images",
      "styles": "_styles",
      "scripts": "_scripts",
      "videos": "_videos",
      "data": "_data"
    },
    "entries": {
      "js": "main**.js",
      "css": "main**.less"
    }
  }
}
